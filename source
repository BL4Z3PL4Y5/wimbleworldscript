EXECUTE.BackgroundColor3 = Color3.fromRGB(48, 48, 48)
EXECUTE.BorderColor3 = Color3.fromRGB(26, 26, 26)
EXECUTE.BorderSizePixel = 4
EXECUTE.Position = UDim2.new(0.0652173907, 0, 0.479999989, 0)
EXECUTE.Size = UDim2.new(0, 200, 0, 50)
EXECUTE.Font = Enum.Font.GothamMedium
EXECUTE.Text = "EXECUTE"
EXECUTE.TextColor3 = Color3.fromRGB(170, 0, 0)
