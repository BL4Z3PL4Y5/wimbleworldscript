local ScreenGui = Instance.new("ScreenGui")
local FRAME = Instance.new("Frame")
local TITLE = Instance.new("TextLabel")
local TEXTUPPER = Instance.new("TextLabel")
local EXECUTE = Instance.new("TextButton")
local TEXTBOTTOM = Instance.new("TextLabel")

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

FRAME.Name = "FRAME"
FRAME.Parent = ScreenGui
FRAME.BackgroundColor3 = Color3.fromRGB(63, 63, 63)
FRAME.BorderColor3 = Color3.fromRGB(40, 40, 40)
FRAME.BorderSizePixel = 3
FRAME.Position = UDim2.new(0.414306998, 0, 0.391676933, 0)
FRAME.Size = UDim2.new(0, 230, 0, 175)
FRAME.Active=true
FRAME.Draggable=true

TITLE.Name = "TITLE"
TITLE.Parent = FRAME
TITLE.BackgroundColor3 = Color3.fromRGB(53, 53, 53)
TITLE.BorderColor3 = Color3.fromRGB(40, 40, 40)
TITLE.BorderSizePixel = 3
